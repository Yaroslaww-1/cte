    "@nestjs/testing": "^7.5.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "jest": "^26.6.3",
