  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/backend/src/main",
    "test:unit": "jest --config ./test/unit/jest.unit.json --verbose --maxWorkers=2",
    "test:unit:watch": "jest --config ./test/unit/jest.unit.json --watch",
    "test:unit:cov": "jest --config ./test/unit/jest.unit.json --coverage",
    "db:migrate": "knex migrate:latest --knexfile ./src/data/knexfile.ts",
    "db:migrate:create": "npx knex migrate:make --knexfile ./src/data/knexfile.ts -x ts",
    "db:migrate:win": "npx knex migrate:latest --knexfile .\\src\\data\\knexfile.ts -x ts",
    "db:migrate:create:win": "npx knex migrate:make --knexfile .\\src\\data\\knexfile.ts -x ts"
  },
  "dependencies": {
    "@nestjs/cli": "^7.5.1",
