    "@nestjs/schematics": "^7.1.3",
    "@nestjs/websockets": "^7.6.13",
    "@types/node": "^14.14.6",
    "@types/ws": "^7.4.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.18",
    "objection": "^2.2.15",
